
public class Student {
	public String firstName;
	public String lastName;	
	public double score;
	
	// Default Constructor
	// Constructor should be public method and the same name as class name
	public Student(String fName, String lName, double studentScore) {
		firstName = fName;
		lastName = lName;
		score = studentScore;		
	}
	
	public double getScore() {
		return 0.0;
	}
}
